{
  "manifestVersion": "v3",
  "name": "SampleApp",
  "interfaces": [
    {
      "type": "vehicle-signal-interface",
      "config": {
        "src": "https://github.com/eclipse-kuksa/kuksa-common/blob/main/vss/vss_release_4.1.json",
        "unit_src": [
          "https://github.com/COVESA/vehicle_signal_specification/blob/v4.1/spec/units.yaml"
        ],
        "datapoints": {
          "required": [
            {
              "path": "Vehicle.Cabin.Door.Row1.DriverSide.IsLocked",
              "access": "write"
            },
            {
              "path": "Vehicle.Cabin.HVAC.Fronthousing.AchmcAcOnStatusS",
              "access": "read"
            },
            {
              "path": "Vehicle.Cabin.HVAC.Fronthousing.AcbCloseButton",
              "access": "write"
            }
          ]
        }
      }
    },
    {
      "type": "grpc-interface",
      "config": {
        "src": "src/proto/door.proto",
        "provided": {}
      }
    },
    {
      "type": "grpc-interface",
      "config": {
        "src": "src/proto/fronthousing.proto",
        "provided": {}
      }
    },
    {
      "type": "lattice",
      "config": {}
    }
  ]
}
